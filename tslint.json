{
  "defaultSeverity": "warn",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "prefer-const": false,
    "interface-name": [true, "never-prefix"],
    "only-arrow-functions": false,
    "no-bitwise": false
  }
}
