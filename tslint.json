{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "arrow-parens": false,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "member-access": false,
        "max-line-length": [
            true,
            100
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-console": false,
        "prefer-const": false,
        "interface-name": [true, "never-prefix"],
        "only-arrow-functions": false,
        "no-bitwise": false
    },
    "rulesDirectory": []
}
