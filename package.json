{
  "name": "assignpro-frontend",
  "version": "0.1.0",
  "description": "Frontend for AssignPro webapp",
  "author": "Paweł Dąbrowski <dabrowskip9@gmail.com>",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier src/**/*.* --write",
    "start": "vue-cli-service serve",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@handsontable/vue": "^3.1.0",
    "axios": "^0.18.0",
    "buefy": "^0.6.6",
    "firebase": "^5.7.1",
    "handsontable": "^6.2.2",
    "intro.js": "^2.9.3",
    "lodash-es": "^4.17.11",
    "mdi": "^2.2.43",
    "numbro": "^2.1.1",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.5.21",
    "vue-class-component": "^6.3.2",
    "vue-i18n": "^8.5.0",
    "vue-introjs": "^1.3.2",
    "vue-property-decorator": "^7.2.0",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@types/jest": "^23.3.10",
    "@types/lodash-es": "^4.17.1",
    "@types/webpack": "^4.4.22",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-e2e-cypress": "^3.2.0",
    "@vue/cli-plugin-typescript": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.11.0",
    "prettier": "1.15.3",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.10.5",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2",
    "vue-cli-plugin-i18n": "^0.5.1",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.2.0",
    "vue-template-compiler": "^2.5.21"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not ie 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  },
  "bugs": {
    "url": "https://github.com/paolostyle/assignpro/issues"
  },
  "homepage": "https://github.com/paolostyle/assignpro#readme",
  "keywords": [
    "assignpro",
    "assignment-problem",
    "vue",
    "typescript",
    "webpack",
    "sass"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paolostyle/assignpro.git"
  }
}
